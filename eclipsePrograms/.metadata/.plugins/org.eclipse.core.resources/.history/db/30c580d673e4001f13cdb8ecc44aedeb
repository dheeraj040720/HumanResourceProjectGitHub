
public class average {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	//wap from 100 to 1
		                                                        // 100   
		for(int i=1;i<10;i++) //   i = 2     
		{
			
		   i+=i+1;
			
			System.out.println(i);
		}
		
		
		
	}

}
